{
  "map_enabled": true,
  "auto_unpin_on_clear": true,
  "disabled_image_filter": "grayscale, dim",
  "chest_unopened_img": "images/BasicChestClosed.png",
  "chest_opened_img": "images/BasicChestOpen.png",
  "layouts": {
    "shared_item_grid": {
      "type": "array",
      "orientation": "vertical",
      "content": [
        {
          "type": "itemgrid",
          "h_alignment": "center",
          "item_margin": "0,0,4,15",
          "item_size": 48,
          "rows": [ 
            [ "telescope", "sail", "windwaker", "grapple", "spoils", "boomerang", "leaf", "bombs", "sword" ],
            [ "tuner", "pic1", "boots", "armor", "baitbag", "bow", "elementals", "lightarrow", "shield" ],
            [ "bottle", "magletter", "moeletter", "momletter", "delivery", "hookshot", "hammer", "wallet", "power" ],
            [ "deed", "requiem", "ballad", "command", "lyric", "aria", "passing", "gschart" ],
          ]
        }
      ]
    },
    "toggles": {
      "type": "array",
      "orientation": "horizontal",
      "h_alignment": "center",
      "content": [
        {
          "type": "array",
          "orientation": "vertical",
          "h_alignment": "center",
          "content": [
            {
              "type": "item",
              "item": "dungeons"
            },
            {
              "type": "item",
              "item": "sidequests"
            },
            {
              "type": "item",
              "item": "platforms"
            },
            {
              "type": "item",
              "item": "reefs"
            },
            {
              "type": "item",
              "item": "triforcecharts"
            },
            {
              "type": "item",
              "item": "gifts"
            },
            {
              "type": "item",
              "item": "expensive"
            }
          ]
        },
        {
          "type": "array",
          "orientation": "vertical",
          "h_alignment": "center",
          "content": [
            {
              "type": "item",
              "item": "caves"
            },
            {
              "type": "item",
              "item": "minigames"
            },
            {
              "type": "item",
              "item": "subs"
            },
            {
              "type": "item",
              "item": "octos"
            },
            {
              "type": "item",
              "item": "treasurecharts"
            },
            {
              "type": "item",
              "item": "mail"
            },
            {
              "type": "item",
              "item": "misc"
            }
          ]
        }
      ]
    },
    "tracker_default": {
      "type": "container",
      "background": "#00FF74",
      "content": {
        "type": "dock",
        "dropshadow": true,
        "content": [
          {
            "type": "dock",
            "dock": "left",
            "v_alignment": "stretch",
            "content": [
              {
                "type": "group",
                "header": "Items",
                "dock": "top",
                "content": {
                  "type": "layout",
                  "key": "shared_item_grid"
                }
              },
              {
                "type": "group",
                "header": "Pinned Locations",
                "content": {
                  "type": "recentpins",
                  "style": "wrap",
                  "h_alignment": "stretch",
                  "v_alignment": "stretch",
                  "orientation": "vertical",
                  "compact": true
                }
              }
            ]
          },
          {
            "type": "tabbed",
            "tabs": [
              {
                "title": "Sea Chart",
                "icon": "images/Dungeon Map.png",
                "content": {
                  "type": "map"
                }
              },
              {
                "title": "Settings",
                "icon": "images/gear.png",
                "content": {
                  "type": "layout",
                  "key": "toggles"
                }
              }
            ]
          }
        ]
      }
    },
    "tracker_horizontal": {
      "type": "container",
      "background": "#00FF74",
      "content": {
        "type": "dock",
        "dropshadow": true,
        "content": [
          {
            "type": "dock",
            "dock": "bottom",
            "content": [
              {
                "type": "group",
                "header": "Items",
                "margin": "0,0,3,0",
                "dock": "left",
                "content": {
                  "type": "layout",
                  "key": "shared_item_grid"
                }
              },
              {
                "type": "group",
                "header": "Pinned Locations",
                "content": {
                  "type": "recentpins",
                  "style": "wrap",
                  "h_alignment": "stretch",
                  "v_alignment": "stretch",
                  "orientation": "horizontal",
                  "compact": true
                }
              }
            ]
          },
          {
            "type": "tabbed",
            "tabs": [
              {
                "title": "Sea Chart",
                "icon": "images/Dungeon Map.png",
                "content": {
                  "type": "map"
                }
              },
              {
                "title": "Settings",
                "icon": "images/gear.png",
                "content": {
                  "type": "layout",
                  "key": "toggles"
                }
              }
            ]
          }
        ]
      }
    }
  }
}
