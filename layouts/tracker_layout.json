{
  "shared_item_grid": {
    "type": "array",
    "orientation": "vertical",
    "content": [
      {
        "type": "itemgrid",
        "h_alignment": "center",
        "item_margin": "0,0,4,15",
        "item_size": 48,
        "rows": [
          [ "telescope", "sail",      "windwaker", "grapple",   "spoils",   "boomerang", "leaf",   "double",   "no_sword"  ],
          [ "tuner",     "pic1",      "boots",     "armor",     "baitbag",  "no_bow",    "bombs",  "wallet",   "no_shield" ],
          [ "bottle",    "magletter", "moeletter", "momletter", "delivery", "hookshot",  "hammer", "no_trade", "power"     ],
          [ "requiem",   "ballad",    "command",   "lyric",     "aria",     "passing",   "deed",   "gschart",  "statue1"   ]
        ]
      },
      {
        "type": "array",
        "orientation": "horizontal",
        "h_alignment": "center",
        "content": [
          {
            "type": "itemgrid",
            "h_alignment": "center",
            "item_margin": "0,0,10,10",
            "item_size": 32,
            "rows": [
              [ "no_quiver" ],
              [ "no_bomb" ],
              [ "hurricane" ]
            ]
          },
          {
            "type": "layout",
            "key": "pearls"
          },
          {
            "type": "layout",
            "key": "dungeon_keys"
          },
          {
            "type": "layout",
            "margin": "10,0,0,0",
            "key": "triforce"
          }
        ]
      }
    ]
  },
  "tracker_capture_item": {
    "type": "layout",
    "key": "shared_item_grid"
  },
  "tracker_default": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "shared_item_grid"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "vertical",
                "width": 488,
                "compact": true
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Sea Chart",
              "icon": "images/Dungeon Map.png",
              "content": {
                "type": "map"
              }
            },
            {
              "title": "Settings",
              "icon": "images/gear.png",
              "content": {
                "type": "layout",
                "key": "toggles"
              }
            }
          ]
        }
      ]
    }
  }
}
